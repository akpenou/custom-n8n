    "promise-retry": "^2.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sharp": "^0.33.3",
    "tmp": "0.2.1",
    "tmp-promise": "^3.0.2",
    "turndown": "^7.1.3",
    "url": "^0.11.3",
    "uuid": "^8.3.2",
    "youtube-transcript": "^1.0.6"
  }
}
